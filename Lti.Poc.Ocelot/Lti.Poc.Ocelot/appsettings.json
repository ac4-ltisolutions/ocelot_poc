{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SwaggerEndPoints": [
    {
      "Key": "weather",
      "TakeServersFromDownstreamService": false,
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Weather API",
          "Version": "v1",
          "Url": "http://localhost:5580/swagger/v1/swagger.json"
        }
      ]

    },
    {
      "Key": "sometext",
      "TakeServersFromDownstreamService": false,
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Text API",
          "Version": "v1",
          "Url": "http://localhost:5680/swagger/v1/swagger.json"
        }
      ]

    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": null,
    "DownstreamScheme": "http",
    "ServiceDiscoveryProvider": {
      "Scheme": "http",
      "Host": "localhost",
      "Port": 8055,
      "Type": "LtidServiceDiscoveryProvider"
    }
  }
}
