{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Routes": [
    {
      "DownstreamPathTemplate": "/WeatherForecast",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5581
        }
      ],
      "UpstreamPathTemplate": "/api/weather/WeatherForecast",
      "UpstreamHttpMethod": [ "Get" ],
      "SwaggerKey": "weather"
    },
    {
      "DownstreamPathTemplate": "/SomeText",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5681
        }
      ],
      "UpstreamPathTemplate": "/api/sometext/SomeText",
      "UpstreamHttpMethod": [ "Get" ],
      "SwaggerKey": "weather"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "weather",
      "TakeServersFromDownstreamService": true,
      "Config": [
        {
          "Name": "Contacts API",
          "Version": "v1",
          "Url": "https://localhost:5581/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:44335"
  }
}
